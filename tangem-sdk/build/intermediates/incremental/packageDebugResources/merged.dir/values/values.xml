    <style name="SdkBottomSheetDialogStyle" parent="Theme.MaterialComponents.Light.BottomSheetDialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowSoftInputMode">adjustResize</item>
    </style>
    <style name="SdkTextView" parent="Widget.MaterialComponents.TextView">
        <item name="android:textColor">@color/card_sdk_text_color_primary</item>
    </style>
    <style name="SdkTextView.Medium">
        <item name="fontFamily">sans-serif-medium</item>
    </style>
