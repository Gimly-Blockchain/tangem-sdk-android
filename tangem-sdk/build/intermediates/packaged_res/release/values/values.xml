        <attr format="boolean" name="pb_is_indeterminate"/>
        <attr format="integer" name="pb_progress"/>
        <attr format="integer" name="pb_progress_max"/>
        <attr format="integer" name="pb_sweep_step"/>
        <attr format="integer" name="pb_sweep_start_increment"/>
        <attr format="integer" name="pb_sweep_start_decrement"/>
        <attr format="color" name="pb_color"/>
        <attr format="color" name="pb_secondary_color"/>
        <attr format="dimension" name="pb_thickness"/>
        <attr format="dimension" name="pb_secondary_thickness"/>
        <attr format="boolean" name="pb_round_border"/>
    </declare-styleable>
